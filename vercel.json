{
  "version": 2,
  "buildCommand": "cd Backend && npm install && npm run build",
  "installCommand": "cd Backend && npm install",
  "functions": {
    "Backend/src/server.ts": {
      "runtime": "@vercel/node@3.0.23"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/Backend/src/server.ts"
    }
  ]
}
